{
  "name": "msw-auto-mock",
  "version": "0.5.2",
  "description": "Generate random mock data from OpenAPI descriptions for msw.",
  "main": "dist/cli.js",
  "module": "./dist/cli.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "require": "./dist/cli/index.js",
      "import": "./dist/cli/index.mjs",
      "types": "./dist/cli/index.d.ts"
    }
  },
  "bin": {
    "msw-auto-mock": "./bin/cli/index.js"
  },
  "scripts": {
    "build": "tsup",
    "fmt": "prettier {example,src}/**/*.{js,jsx,ts,tsx,css,md,html} --write",
    "test": "vitest run"
  },
  "author": "zoubingwu<zoubingwu@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.3",
    "cac": "^6.7.12",
    "msw": "^0.36.3",
    "oazapfts": "^3.5.0",
    "prettier": "2.5.1",
    "swagger2openapi": "^7.0.8"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.5",
    "@types/prettier": "^2.4.2",
    "lodash": "^4.17.21",
    "openapi-types": "^10.0.0",
    "tsup": "^5.11.9",
    "typescript": "^4.5.4",
    "vitest": "^0.0.132"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoubingwu/msw-auto-mock.git"
  },
  "bugs": {
    "url": "https://github.com/zoubingwu/msw-auto-mock/issues"
  },
  "homepage": "https://github.com/zoubingwu/msw-auto-mock",
  "files": [
    "dist"
  ],
  "keywords": [
    "msw",
    "mock",
    "service-worker",
    "openapi"
  ]
}
